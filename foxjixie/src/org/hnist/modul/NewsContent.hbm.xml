<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.hnist.modul.NewsContent" table="newsContent" catalog="foxjixie">
       
        <id name="ncid" type="java.lang.Integer">
            <column name="ncid" length="15" />
            <generator class="native"></generator>
        </id>
        
         <property name="nhid" type="java.lang.Integer">
            <column name="nhid" length="15"/>
        </property>
        
         <property name="ncContent" type="java.lang.String">
            <column name="ncContent" length="10000"/>
        </property>
        
 
        
        
        
    </class>
</hibernate-mapping>